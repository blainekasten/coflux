{
  "name": "coflux",
  "version": "0.2.0",
  "description": "Component-based Flux",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "start": "npm run build:watch:demos & node ./demos/server.js",
    "lint": "eslint src/**",
    "build:demos": "webpack --config demos/webpack.config.js",
    "build:watch:demos": "webpack --watch --config demos/webpack.config.js",
    "build": "babel src --out-dir lib --ignore tests",
    "test": "jest",
    "upload-coverage": "./node_modules/codecov/bin/codecov -t eb05c6f3-2078-4888-b119-69c3ade9c6dc"
  },
  "files": [
    "src",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/blainekasten/coflux.git"
  },
  "author": "Blaine Kasten <blainekasten@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "invariant": "^2.2.1",
    "object-crawl": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.5.2",
    "babel-eslint": "^4.1.6",
    "babel-jest": "^12.0.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-css-modules-transform": "0.0.7",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "codecov": "^1.0.1",
    "coveralls": "^2.11.4",
    "css-loader": "^0.23.1",
    "enzyme": "^2.2.0",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.3.0",
    "estraverse-fb": "^1.3.1",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "glob": "^7.0.3",
    "istanbul": "^0.4.0",
    "jasmine-enzyme": "^1.0.0",
    "jest-cli": "^12.0.0",
    "mocha": "^2.2.5",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.0",
    "rimraf": "^2.4.3",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14"
  },
  "jest": {
    "collectCoverage": true,
    "setupTestFrameworkScriptFile": "node_modules/jasmine-enzyme/lib/jest.js",
    "testPathDirs": [
      "src"
    ],
    "unmockedModulePathPatterns": [
      "enzyme",
      "glob",
      "object-crawl",
      "react"
    ]
  }
}
